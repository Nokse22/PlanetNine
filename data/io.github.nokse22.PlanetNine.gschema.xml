<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="planetnine">
	<schema id="io.github.nokse22.PlanetNine" path="/io/github/nokse22/PlanetNine/">
	  <key name="notebook-line-number" type="b">
      <default>false</default>
    </key>
	  <key name="code-vim" type="b">
      <default>false</default>
    </key>
	  <key name="code-line-number" type="b">
      <default>true</default>
    </key>
	  <key name="code-highlight-row" type="b">
      <default>true</default>
    </key>
	  <key name="browser-default-url" type="s">
      <default>'https://start.duckduckgo.com/'</default>
    </key>
	  <key name="code-highlight-brackets" type="b">
      <default>true</default>
    </key>
	  <key name="selected-theme" type="s">
      <default>'Adwaita'</default>
    </key>

	  <key name="start-server-immediately" type="b">
      <default>true</default>
    </key>
	  <key name="auto-shutdown-kernel" type="b">
      <default>false</default>
    </key>

	</schema>
</schemalist>
