                            <property name="hexpand">true</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkDropTarget" id="list_drop_target">
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
